{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2016",
      "es2015",
      "dom"
    ],
    "rootDir": "./src",
    "noImplicitAny": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "declaration": true,
    "outDir": "dist",
    "moduleResolution": "node",
  },
  "exclude": [
    "dist/",
    "__tests__/",
  ]
}